{
  "name": "infumap",
  "version": "1.0",
  "description": "Export browser links to an infumap server.",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "contextMenus",
    "cookies",
    "storage"
  ],
  "host_permissions": [
    "https://*/"
  ],
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "action": {
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png"
    },
    "default_title": "bookmark"
  },
  "options_page": "options.html"
}
